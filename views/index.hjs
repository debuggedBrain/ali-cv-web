<script>
function myMap() {
  myCenter=new google.maps.LatLng(40.5008, -74.4474);
  var mapOptions= {
    center:myCenter,
    zoom:12, scrollwheel: false, draggable: false,
    mapTypeId:google.maps.MapTypeId.ROADMAP
  };
  var map=new google.maps.Map(document.getElementById("googleMap"),mapOptions);

  var marker = new google.maps.Marker({
    position: myCenter,
  });
  marker.setMap(map);
}
</script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyASHYxvxfb0AKbvVWhIRMJq9FOvRyQuBMM&callback=myMap"></script>
